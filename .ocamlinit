#require "ppx_deriving.show"
#directory "lib"
#mod_use "state.ml";;
#mod_use "types.ml";;
#mod_use "names.ml";;
#mod_use "graph.ml";;
open Types 
module Float_Graph = Graph.Make_Graph(Float_t)

let _ = Names.with_fresh @@ (fun _ ->
  let open Float_Graph in
  let a = value_of 1.0  in
  let two = const 2.0 in
  let c = two * a in 
  let nodes = backward c in
  print_endline @@ [%show: (string * t) list] nodes
)
